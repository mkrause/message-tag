{
    "name": "message-tag",
    "version": "0.1.4",
    "description": "ES6 template literal tag to format message strings on arbitrary inputs",
    "keywords": [
        "message",
        "error",
        "log",
        "format",
        "template",
        "tag",
        "tagged",
        "es6-tag"
    ],
    "author": "mkrause",
    "license": "MIT",
    "repository": "https://github.com/mkrause/message-tag",
    "es2015": "src/index.js",
    "main": "lib-cjs/index.js",
    "module": "lib-esm/index.js",
    "browser": "lib-esm/index.js",
    "files": [
        "src/",
        "lib-cjs/",
        "lib-esm/"
    ],
    "scripts": {
        "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel src --out-dir lib-cjs --source-maps inline",
        "build:esm": "NODE_ENV=production BABEL_ENV=esm babel src --out-dir lib-esm --source-maps inline",
        "test": "NODE_ENV=development BABEL_ENV=cjs mocha --require babel-register --recursive tests",
        "prepublishOnly": "npm run build:esm && npm run build:cjs"
    },
    "devDependencies": {
        "babel-core": "^7.0.0-beta.3",
        "babel-register": "^7.0.0-beta.3",
        "babel-cli": "^7.0.0-beta.3",
        "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
        "babel-preset-env": "^7.0.0-beta.3",
        "mocha": "^5.0.4",
        "chai": "^4.1.2"
    },
    "dependencies": {}
}
